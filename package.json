{
	"name": "revanza-web-porto",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"build:memory": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npm run build",
		"preview": "astro preview",
		"astro": "astro",
		"check": "biome lint .",
		"check:fix": "biome check --write .",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "node --test \"tests/unit/*.test.js\"",
		"test:integration": "node --test \"tests/integration/*.test.js\"",
		"test:monitoring": "playwright test tests/monitoring/client-errors.spec.js --reporter=line",
		"docker:dev": "docker compose up --build",
		"docker:dev:down": "docker compose down --remove-orphans",
		"sanity": "sanity dev",
		"sanity:dev": "sanity dev",
		"sanity:deploy": "sanity deploy",
		"sanity:build": "sanity build"
	},
	"devDependencies": {
		"@astrojs/check": "^0.6.0",
		"@astrojs/tailwind": "^5.1.0",
		"@biomejs/biome": "1.7.3",
		"@playwright/test": "^1.58.2",
		"@sanity/cli": "^3.88.3",
		"@tailwindcss/typography": "^0.5.13",
		"@types/rss": "^0.0.32",
		"astro": "^4.8.2",
		"cross-env": "^7.0.3",
		"sanity": "^3.88.3",
		"tailwindcss": "^3.4.3",
		"terser": "^5.41.0",
		"typescript": "^5.4.5",
		"vercel": "^44.6.3"
	},
	"packageManager": "npm@11.3.0",
	"dependencies": {
		"@mlc-ai/web-llm": "^0.2.79",
		"@portabletext/to-html": "^2.0.14",
		"@sanity/client": "^7.2.1",
		"@sanity/image-url": "^1.1.0",
		"rss": "^1.2.2"
	}
}
